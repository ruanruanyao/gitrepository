<struts>
	<package name="address" extends="struts-default" namespace="/address">
		<action name="*" class="com.baizhi.action.AddressAction" method="{1}">
			<result name="toLogin" type="redirect">/front/user/login_form.jsp</result>
			<result name="toAddress" type="redirect">/front/order/address_form.jsp</result>
			<result name="toAddr">/front/order/address_form.jsp</result>
		</action>
	</package>
</struts>
